import java.io.*;
import java.util.*;

public class Main {

    public static void main(String[] args) throws Exception {
        Scanner scn = new Scanner (System.in);
        int n = scn.nextInt();
        printDecInc(n);
    }

    public static void printDecInc(int n)
    {
        if(n == 0)
        {
            return;
        }
        
        //meeting expectations with faith
        System.out.println(n);
        //faith
        printDecInc(n-1);
         //meeting expectations with faith
        System.out.println(n);
    }

}
